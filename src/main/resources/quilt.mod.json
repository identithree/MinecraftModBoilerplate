{
	"schema_version": 1,
	"quilt_loader": {
		"group": "dev.quinnlane",
		"id": "mod",
		"version": "${version}",
		"metadata": {
			"name": "Minecraft Mod Boilerplate",
			"description": "This is a boilerplate mod built with Quilt and Kotlin.",
			"contributors": {
				"Quinn Lane": "Owner"
			},
			"contact": {
				"homepage": "https://quinnlane.dev/",
				"sources": "https://gitlab.com/Identithree/mc-modbp",
				"issues": "https://gitlab.com/Identithree/mc-modbp/issues"
			},
			"icon": "assets/mod/icon.png",
			"license": "CC0-1.0"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "dev.quinnlane.mod.Main"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.1-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=2.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19"
			}
		]
	},
	"mixin": "mod.mixins.json"
}
